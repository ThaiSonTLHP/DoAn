                <label class="form-check-label">
                    <input class="form-check-input" asp-for="XacNhan" /> @Html.DisplayNameFor(model => model.XacNhan)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
